pipeline {
    agent any
    
    tools {
        jdk 'JDK11'
        maven 'Maven'
    }
    stages {
        stage('git checkout'){
            steps{
                git branch: 'main', url: 'https://github.com/praveenkumarchannappagoudra/test-1.git'
            }
        }
        stage('compile'){
            steps{
                sh "mven compile"
            }
        }
        stage('build'){
            steps{
                sh "mvn pacakge"
            }
        }
    }

    }

